{
    "name": "icemix/laravel-passport-social-grant",
    "description": "Icemix Social Grant for Laravel Passport",
    "keywords": ["laravel","Icemix", "passport", "social", "network", "grant", "oauth"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Etta Egbe Joseph",
            "email": "ettaegbe@icemix.net"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Icemix\\SocialGrant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icemix\\SocialGrant\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Icemix\\SocialGrant\\Providers\\SocialGrantServiceProvider"
            ]
        }
    }
}
